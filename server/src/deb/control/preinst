#!/bin/bash
echo "Preinstall"
adduser piuser
mkdir /opt/pihomesecurityserver
chown piuser:piuser /opt/pihomesecurityserver
touch /var/log/pihomesecurityserver.log
chown piuser:piuser /var/log/pihomesecurityserver.log
touch /var/log/pihomesecurityserver.out
chown piuser:piuser /var/log/pihomesecurityserver.out
